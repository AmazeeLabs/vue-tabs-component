{
  "name": "@spatie/vue-tabs",
  "version": "0.0.1",
  "description": "A Vue component to easily render tabs",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --config demo/webpack.config.js",
    "build": "rm -rf dist && NODE_ENV=production webpack --config webpack.dist.js",
    "lint": "eslint --fix src __tests__; exit 0",
    "prepublish": "npm run test; npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spatie/vue-tabs.git"
  },
  "keywords": [
    "spatie"
  ],
  "author": "Freek Van der Herten",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spatie/vue-tabs/issues"
  },
  "homepage": "https://github.com/spatie/vue-tabs",
  "peerDependencies": {
    "vue": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "eslint": "^3.7.1",
    "eslint-config-spatie": "^1.1.0",
    "jest": "^19.0.0",
    "jest-vue-preprocessor": "^0.2.0",
    "vue": "^2.0.3",
    "vue-loader": "^12.0.3",
    "vue-template-compiler": "^2.3.2",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "jest": {
    "testRegex": "test.js$",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    }
  },
  "dependencies": {
    "babel-preset-env": "^1.4.0"
  }
}
